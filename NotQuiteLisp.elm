module NotQuiteLispWhere where

import Debug
import Html
import String

-- fold :  -> Int -> Int
fold acc l =
  if l == "(" then
     acc + 1
   else
     acc - 1


main : Html.Html
main = 
  input
    |> String.toList
    |> List.map (\n -> if n == '(' then 1 else -1)
    |> List.foldr (+) 0
    |> toString
    |> Html.text

